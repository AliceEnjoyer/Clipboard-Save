      vbl->addWidget(lbl);
            dio->setLayout(vbl);
            dio->exec();

            delete txt;
            delete lbl;
            delete vbl;
            delete dio;
        } else if (i == "Copy") {
            qApp->clipboard()->setText(CBIModel->getCBI(currentIndex()).Data());
        } else if (i == "Delete") {
            CBIModel->deleteCBI(currentIndex().row());
        }